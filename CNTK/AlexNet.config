WorkDir=.
ModelDir=$WorkDir$/_out/$ConfigName$
stderr=$WorkDir$/_out/$ConfigName$

ndlMacros=$WorkDir$/Macros.ndl

precision=float
deviceId=Auto

command=Train

makeMode=false

parallelTrain=false

prefetch=true

traceLevel=1

Train=[
    action=train
    modelPath=$ModelDir$/AlexNet

    NDLNetworkBuilder=[
        networkDescription=$WorkDir$/AlexNet.ndl
    ]
    
    SGD=[
        epochSize=2048
        minibatchSize=256
        learningRatesPerMB=0.01
        momentumPerMB=0
        maxEpochs=10
        gradUpdateType=None
        L2RegWeight=0
        dropoutRate=0
        
        ParallelTrain=[
            parallelizationMethod=DataParallelSGD
            distributedMBReading=true
            parallelizationStartEpoch=1
            DataParallelSGD=[
                gradientBits=1
            ]
        ]

        numMBsToShowResult=4
    ]

reader=[
    readerType=UCIFastReader
    file=$WorkDir$/imagenet_data.txt
    features=[
        dim=150528
        start=1
    ]
    labels=[
        dim=1
        start=0
	labelDim=1000
	labelMappingFile=$WorkDir$/labelmap.txt
    ]
]
    
]
